{
	"tabWidth": 2,
	"useTabs": true,
	"trailingComma": "all",
	"singleQuote": false,
	"semi": true,
	"printWidth": 100,
	"proseWrap": "always",
	"bracketSpacing": true,
	"quoteProps": "as-needed",
	"singleAttributePerLine": true,
	"arrowParens": "always",
	"svelteSortOrder": "scripts-markup-styles",
	"pluginSearchDirs": ["."],
	"overrides": [{ "files": "*.svelte", "options": { "parser": "svelte" } }]
}
